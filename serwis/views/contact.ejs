<script src="js/dependencies/jquery/jquery-2.1.4.min.js"></script>
<script src="js/bootstrap/bootstrap-3.3.5.min.js"></script>
<link rel="stylesheet" type="text/css" href="/bootstrap/css/bootstrap.css">
<html>
<head>
  <title> Contact </title>
  <body>
<% if(sent){ %>
<form action="/contact" method="post">
  <div class="form-group" name="message">
    <label for="message">Another label</label>
    <input type="text" class="form-control" name="message" id="message" placeholder="Type your message">
  </div>
  <div class="form-group" name="order">
  <label for="order">Which order do you want to ask about?</label>
  <select name="orderId">
    <div>
  <option value="">Choose </option>
    <% orders.forEach(function(order) { %>
    <option value="<%= order.id %>"><%= order.product %></option>
    <% }); %>
      </div>
  </select>
  </div>
  <div class="form-group">
    <input type="submit" value="Send">
  </div>
</form>
<% } else{ %>
<h1 class="header1">Wyslano! </h1>
<% } %>
